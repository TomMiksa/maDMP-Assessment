@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix dcso: <https://w3id.org/dcso/ns/core#>.
@prefix validation: <https://w3id.org/validation/ns/core#>.

validation:oneShape a sh:NodeShape ;
	sh:targetObjectsOf dcso:hasDataset ;
	sh:property [
					sh:path dcso:dataQualityAssurance ;
					sh:minCount 1 ;
				] .

validation:twoShape a sh:NodeShape ;
	sh:targetObjectsOf dcso:hasHost ;
	sh:property [
					sh:path dcso:availability ;
					sh:minCount 1 ;
				] .